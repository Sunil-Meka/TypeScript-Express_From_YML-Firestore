{
  "name": "@openapi-generator-plus/typescript-express-passport-server-generator",
  "version": "2.4.0",
  "description": "An OpenAPI Generator+ module for an Express + Passport API server in TypeScript",
  "keywords": [
    "openapi-generator-plus",
    "openapi-generator-plus-generator",
    "openapi",
    "openapi-generator",
    "typescript",
    "express",
    "passport",
    "server"
  ],
  "main": "dist/index.js",
  "author": "Karl von Randow",
  "license": "Apache-2.0",
  "dependencies": {
    "@openapi-generator-plus/core": "^0.42.0",
    "@openapi-generator-plus/generator-common": "^0.26.0",
    "@openapi-generator-plus/handlebars-templates": "^0.27.2",
    "@openapi-generator-plus/indexed-type": "^0.1.6",
    "@openapi-generator-plus/testing": "^0.42.0",
    "@openapi-generator-plus/types": "^0.42.0",
    "@openapi-generator-plus/typescript-generator-common": "^0.25.7",
    "openapi-generator-plus": "^0.42.0"
  },
  "peerDependencies": {
    "@openapi-generator-plus/core": "^0.42.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.18.1",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlvr/openapi-generator-plus-express-passport.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "rimraf dist",
    "link-openapi-generator-plus": "pnpm link --global openapi-generator-plus $(cd node_modules && find @openapi-generator-plus -depth 1 -prune)",
    "release": "pnpm clean && pnpm build && pnpm test && pnpx changeset publish && pnpm install",
    "release:version": "pnpm changeset version && pnpm install",
    "test": "jest",
    "watch": "tsc --watch"
  }
}